# Generated by django template svn.config


# This ought to be SVN shares

<Location /svn/R3TOS>
        DAV svn
        SVNPath /var/svn/R3TOS
        AuthType Digest
        AuthName Login
        AuthDigestDomain /svn/ https://rfhete470.hs-regensburg.de/svn/R3TOS
        AuthDigestProvider file
        AuthUserFile /etc/apache2/passwd.dav
        AuthGroupFile /etc/apache2/groups.dav
        <LimitExcept OPTIONS>
                Require group svn_r3tos 
        </LimitExcept>
        SSLRequireSSL
</Location>

