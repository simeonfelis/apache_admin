# Generated by django template git.config


SetEnv GIT_PROJECT_ROOT /var/www/git
SetEnv GIT_HTTP_EXPORT_ALL

ScriptAlias /git/ /usr/lib/git-core/git-http-backend/


<Location /git/ipcore>
        Dav on
        Options Indexes
        AuthType Digest
        AuthName Login
        AuthDigestDomain /git/ https://rfhete470.hs-regensburg.de/git/ipcore/
        AuthDigestProvider file
        AuthUserFile /etc/apache2/passwd.dav
        AuthGroupFile /etc/apache2/groups.dav
        Require group git_s3op
</Location>


<Location /git/las3-django>
        Dav on
        Options Indexes
        AuthType Digest
        AuthName Login
        AuthDigestDomain /git/ https://rfhete470.hs-regensburg.de/git/las3-django/
        AuthDigestProvider file
        AuthUserFile /etc/apache2/passwd.dav
        AuthGroupFile /etc/apache2/groups.dav
        Require group git_las3-django
</Location>




